{
  "name": "@antv/f6-plugin",
  "version": "1.0.5",
  "description": "F6 plugin",
  "keywords": [
    "antv",
    "f6",
    "graph",
    "graph analysis",
    "graph editor",
    "graph visualization",
    "relational data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/F6.git"
  },
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "files": [
    "package.json",
    "es",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/f6Plugin.js",
  "miniprogram": "dist",
  "types": "lib/index.d.ts",
  "bugs": {
    "url": "https://github.com/antvis/F6/issues"
  },
  "homepage": "https://github.com/antvis/F6#readme",
  "scripts": {
    "clean": "rimraf es esm lib dist",
    "build:commonjs": "swc src -d lib --config-file ../../.swcrc --source-maps",
    "build:esm": "swc src -d esm --config-file ../../.swcrc --source-maps -C module.type=\"es6\"",
    "build:umd": "webpack --env moduleType=umd --config ../../webpack.config.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:esm && npm run build:umd"
  },
  "dependencies": {
    "@antv/f6-core": "*",
    "@antv/f6-ui": "^1.0.0",
    "@antv/g-mobile": "^0.0.13",
    "@antv/scale": "^0.4.16",
    "@antv/util": "~2.0.5"
  }
}
